<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ljj.myfrescodemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".basicusage.SimpleShowActivity"
            android:label="@string/title_activity_simple_show"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".basicusage.FadeDurationActivity"
            android:label="@string/title_activity_fade_duration" />
        <activity
            android:name=".basicusage.CircleActivity"
            android:label="@string/title_activity_circle"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".basicusage.AbnormalActivity"
            android:label="@string/title_activity_abnormal" />
        <activity
            android:name=".basicusage.ProgressBarActivity"
            android:label="@string/title_activity_progress_bar" />
        <activity
            android:name=".basicusage.OverlayShowActivity"
            android:label="@string/title_activity_overlay_show" />
        <activity
            android:name=".basicusage.GifImageActivity"
            android:label="@string/title_activity_gif_image" />
        <activity
            android:name=".basicusage.LowResolutionActivity"
            android:label="@string/title_activity_low_resolution" />
        <activity android:name=".postprocessor.PostprocessorActivity" />
        <activity
            android:name=".photoview.PhotoViewActivity"
            android:label="@string/title_activity_photo_view"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>